{
  "family": "your-app-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "your-app-container",
      "image": "your-username/your-app-name:latest",
      "portMappings": [
        {
          "containerPort": 8501,
          "hostPort": 8501,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
